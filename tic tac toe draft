#include <iostream>
using namespace std;

bool checkWin(char board[3][3],string playerx,string playero){

    if(board[0][0]=='x'&& board[0][1]==board[0][2] && board[0][2]==board[0][0]){
        cout<<playerx<<" wins!"<<endl;;
    }
    if(board[0][0]=='x'&& board[1][1]==board[2][2] && board[2][2]==board[0][0]){
        cout<<playerx<<" wins!"<<endl;;
    }
    if(board[0][0]=='x'&& board[1][0]==board[2][0] && board[2][0]==board[0][0]){
        cout<<playerx<<" wins!"<<endl;;
    }
    if(board[1][0]=='x'&& board[1][1]==board[1][2] && board[1][2]==board[1][0]){
        cout<<playerx<<" wins!"<<endl;;
    }
    if(board[2][0]=='x'&& board[2][1]==board[2][2] && board[2][2]==board[2][0]){
        cout<<playerx<<" wins!"<<endl;;
    }
    if(board[0][1]=='x'&& board[1][1]==board[2][1] && board[2][1]==board[0][1]){
        cout<<playerx<<" wins!"<<endl;;
    }
    if(board[0][2]=='x'&& board[1][2]==board[2][2] && board[2][2]==board[0][2]){
        cout<<playerx<<" wins!"<<endl;;
    }
    if(board[0][2]=='x'&& board[1][2]==board[2][0] && board[2][0]==board[0][2]){
        cout<<playerx<<" wins!"<<endl;;
    }
    if(board[0][0]=='o'&& board[0][1]==board[0][2] && board[0][2]==board[0][0]){
        cout<<playero<<" wins!"<<endl;
    }
    if(board[0][0]=='o'&& board[1][1]==board[2][2] && board[2][2]==board[0][0]){
        cout<<playero<<" wins!"<<endl;
    }
    if(board[0][0]=='o'&& board[1][0]==board[2][0] && board[2][0]==board[0][0]){
        cout<<playero<<" wins!"<<endl;
    }
    if(board[1][0]=='o'&& board[1][1]==board[1][2] && board[1][2]==board[1][0]){
        cout<<playero<<" wins!"<<endl;
    }
    if(board[2][0]=='o'&& board[2][1]==board[2][2] && board[2][2]==board[2][0]){
        cout<<playero<<" wins!"<<endl;
    }
    if(board[0][1]=='o'&& board[1][1]==board[2][1] && board[2][1]==board[0][1]){
        cout<<playero<<" wins!"<<endl;
    }
    if(board[0][2]=='o'&& board[1][2]==board[2][2] && board[2][2]==board[0][2]){
        cout<<playero<<" wins!"<<endl;
    }
    if(board[0][2]=='o'&& board[1][2]==board[2][0] && board[2][0]==board[0][2]){
        cout<<playero<<" wins!"<<endl;
    }
    else
        return false;

    return true;
}

bool checkTie(char board[3][3]){
    for(int i=0;i<3;i++){
        for(int j=0;j<3;j++){
            if(board[i][j]=='?')
                return false;
        }
    }
    cout<<"its a tie!"<<endl;
    return true;
}

int main(){
    string playerx,playero;
    cin>>playerx;
    cin>>playero;
    bool running=true;
    int i,j;
    char board[3][3];
    for(int i=0;i<3;i++){
        for(int j=0;j<3;j++){
            board[i][j]='?';
        }
    }
    for(int i=0;i<3;i++){
        for(int j=0;j<3;j++){
            cout<<board[i][j]<<" ";
        }
        cout<<endl;
    }

    while(running){
        cout<<playerx<<" play: "<<endl;
        cin>>i;
        cin>>j;
        if(board[i][j]=='?')
            board[i][j]='x';
        for(int i=0;i<3;i++){
            for(int j=0;j<3;j++){
                cout<<board[i][j]<<" ";
            }
            cout<<endl;
        }
        if(checkWin(board,playerx,playero)) {
            running = false;
            break;
        }
        else if(checkTie(board)){
            running=false;
            break;
        }
        cout<<playero<<" play: "<<endl;
        cin>>i;
        cin>>j;
        if(board[i][j]=='?')
            board[i][j]='o';
        for(int i=0;i<3;i++){
            for(int j=0;j<3;j++){
                cout<<board[i][j]<<" ";
            }
            cout<<endl;
        }
        if(checkWin(board,playerx,playero)) {
            running = false;
            break;
        }
        else if(checkTie(board)){
            running=false;
            break;
        }
    }

    return 0;
}
